{
    "name": "gipfl/influxdb",
    "description": "InfluxDB client library",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "gipfl\\InfluxDb\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Thomas Gelf",
            "email": "thomas@gelf.net"
